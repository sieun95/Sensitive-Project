    <link href="./main.css" rel="stylesheet" />
    <link href="./sideNavi.css" rel="stylesheet" />
</head>

<body>
    <%-include('mypageHeader.ejs')%>

        <!-- (가변) Render Area : My Profile -->
        <!-- disabled - User ID -->
        <form action="/modify" method="POST" class="renderdiv listdiv modiv">
            <div class="listHead">내 정보</div>
            <div class="form-group">
                <fieldset disabled="">
                    <label class="form-label" for="disabledInput">ID</label>
                    <input class="form-control" name="modiid" id="disabledInput" type="text" placeholder="<%=userID%>"
                        disabled="">
                </fieldset>
            </div>
            <div class="form-group">
                <fieldset disabled="">
                    <label class="form-label" for="disabledInput">이름</label>
                    <input class="form-control" name="modiid" id="disabledInput" type="text" placeholder="<%=userName%>"
                        disabled="">
                </fieldset>
            </div>
            <div class="form-group">
                <fieldset disabled="">
                    <label class="form-label" for="disabledInput">E-mail 주소</label>
                    <input class="form-control" name="modiid" id="disabledInput" type="text"
                        placeholder="<%=userEmail%>" disabled="">
                </fieldset>
            </div>
            <p></p>
            <input type="submit" class="btn btn-outline-dark" value="Revise" />
            <a href="/logout" class="btn btn-outline-dark">Log Out</a>
        </form>
    </nav>

    <%-include('mypageFooter.ejs')%>


            <!-- Bootstrap core JS-->
            <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/js/bootstrap.bundle.min.js"></script>
            <!-- Core theme JS-->
            <script src="./subscribe.js"></script>
            <!-- * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * *-->
